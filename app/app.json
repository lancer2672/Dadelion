{
  "expo": {
    "name": "Dandelion",
    "slug": "Dandelion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/imgs/Auth.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist":{
        "NSAllowsArbitraryLoads":true
      },
      "usesIcloudStorage ":true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/24.jpg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
